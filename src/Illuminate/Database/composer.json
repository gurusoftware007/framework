{
    "name": "illuminate/database",
    "description": "An elegant database abstraction library.",
    "keywords": ["laravel", "database", "sql", "orm"],
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/events": "dev-master",
        "illuminate/support": "dev-master"
    },
    "require-dev": {
        "illuminate/console": "dev-master",
        "illuminate/filesystem": "dev-master",
        "illuminate/pagination": "dev-master",
        "illuminate/support": "dev-master",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": {
            "Illuminate\\Database": ""
        }
    },
    "target-dir": "Illuminate/Database",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev"
}
